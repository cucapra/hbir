        group[j in cols] col at (0, j)
      }
    }
    group bot_half(bh_rows, bh_cols) at (0 : grid_rows/2, 0 : grid_cols) {
      group[i in bh_rows] row(x, cols) at (i, 0 : bh_cols) {
        group[j in cols] col at (0, j)
      }
    }
  }
}

