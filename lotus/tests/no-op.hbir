// This is the closest to "no-op" possible,
//   given the current parser + compiler combo.
// Currently, the parser requires too much info
target{
    memory g[0]{
        size 8G;
        width 8B;
    };

    tile t[0][0];
}

config{
    group tg[0][0] {
        tile target.t[x][y];
    };
}

data{
    t = 0;

    A: int[dim] = block[target.g.x_max] {
        target.g[x];
        global;
        chunked;
        host;
    };

}

code{
}
